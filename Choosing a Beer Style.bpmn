    <bpmn:sequenceFlow id="Flow_03cwmvk" sourceRef="Activity_0djok1i" targetRef="Gateway_0y059u1" />
    <bpmn:scriptTask id="Activity_0djok1i" name="Create Email Body">
      <bpmn:extensionElements>
        <zeebe:script expression="=&#34;We Could not come up with a beer for you. &#34; + alternative" resultVariable="emailBody" />
