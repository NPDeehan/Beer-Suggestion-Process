  <bpmn:process id="ChooseBeer" name="Choosing a Beer Style" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Give Taste Preferences">
      <bpmn:extensionElements>
        <zeebe:formDefinition formId="Form_0gtp8x4" />
        <zeebe:properties>
          <zeebe:property name="publicAccess" value="true" />
        </zeebe:properties>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_18zy43m</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_18zy43m" sourceRef="StartEvent_1" targetRef="Activity_1e8py6h" />
    <bpmn:sequenceFlow id="Flow_04281se" sourceRef="Activity_1e8py6h" targetRef="Gateway_0bdsl15" />
    <bpmn:endEvent id="Event_0djhxb9" name="Suggestion Given">
      <bpmn:incoming>Flow_1hyyyut</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:businessRuleTask id="Activity_1e8py6h" name="Decide on Beer Style">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="Decision_12ue3dr" resultVariable="beerStyles" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_18zy43m</bpmn:incoming>
      <bpmn:outgoing>Flow_04281se</bpmn:outgoing>
    </bpmn:businessRuleTask>
    <bpmn:sequenceFlow id="Flow_0zj2h9n" sourceRef="Activity_0zisi6k" targetRef="Gateway_0y059u1" />
    <bpmn:sequenceFlow id="Flow_1hyyyut" sourceRef="Activity_0fvzddu" targetRef="Event_0djhxb9" />
    <bpmn:serviceTask id="Activity_0fvzddu" name="Send Email with Beer Suggestions" zeebe:modelerTemplate="io.camunda.connectors.SendGrid.v2" zeebe:modelerTemplateVersion="3" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:sendgrid:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="{{secrets.SendGrid}}" target="apiKey" />
          <zeebe:input source="Niall&#39;s Beer Suggestions" target="from.name" />
          <zeebe:input source="community@camunda.com" target="from.email" />
          <zeebe:input source="=userName" target="to.name" />
          <zeebe:input source="=email" target="to.email" />
          <zeebe:input source="mail" target="unMappedFieldNotUseInModel.mailType" />
          <zeebe:input source="=&#34;Beer Suggestion for &#34;+userName" target="content.subject" />
          <zeebe:input source="text/plain" target="content.type" />
          <zeebe:input source="=emailBody" target="content.value" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0jbypeu</bpmn:incoming>
      <bpmn:outgoing>Flow_1hyyyut</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_0zisi6k" name="Find Beer In Location" zeebe:modelerTemplate="io.camunda.connectors.OpenAI.v1" zeebe:modelerTemplateVersion="5" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" />
        <zeebe:ioMapping>
          <zeebe:input source="bearer" target="authentication.type" />
          <zeebe:input source="{{secrets.OpenAI}}" target="authentication.token" />
          <zeebe:input source="=if is defined(internal_organization) and internal_organization != null then {&#34;Content-Type&#34;:&#34;application/json&#34;, &#34;OpenAI-Organization&#34;:internal_organization} else {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="chat" target="operation" />
          <zeebe:input source="post" target="method" />
          <zeebe:input source="https://api.openai.com/v1/chat/completions" target="url" />
          <zeebe:input source="gpt-3.5-turbo" target="internal_model" />
          <zeebe:input source="1" target="internal_temperature" />
          <zeebe:input source="=&#34;Can you suggest good beers for &#34; + userName +&#34; in the style of &#34; +  string join(beerStyles,&#34;,&#34;) + &#34; that can be found in &#34; + location + &#34; when asked if they have any Dietary Requirements, they said &#34; +dietaryReq" target="internal_prompt" />
          <zeebe:input source="1" target="internal_choices" />
          <zeebe:input source="=append(concatenate(if is defined(internal_systemMessage) then [{&#34;role&#34;: &#34;system&#34;, &#34;content&#34;: internal_systemMessage}] else [], if is defined(internal_chatHistory) then internal_chatHistory else []), {&#34;role&#34;: &#34;user&#34;, &#34;content&#34;: internal_prompt})" target="internal_messages" />
          <zeebe:input source="={&#34;model&#34;: if is defined(internal_custom_model) then internal_custom_model else internal_model, &#34;messages&#34;: internal_messages, &#34;n&#34;: number(internal_choices), &#34;temperature&#34;: number(internal_temperature)}" target="body" />
          <zeebe:input source="30" target="connectionTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={emailBody: response.body.choices[1].message.content}" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0y2q4mi</bpmn:incoming>
      <bpmn:outgoing>Flow_0zj2h9n</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_0bdsl15" name="Was a suitable beer Found?" default="Flow_0y2q4mi">
      <bpmn:incoming>Flow_04281se</bpmn:incoming>
      <bpmn:outgoing>Flow_0y2q4mi</bpmn:outgoing>
